{
    "manifest_version": 2,

    "name": "JumpKey",
    "description": "An extension to allow for a universal keyboard shortcut experience across all webpages.",
    "version": "0.1",
	
	"background": {
		"persistent": true,
		"scripts": ["background.js"]
	},
	
	"content_scripts": [
		{
			"matches":["http://*/*", "https://*/*"], // Injects on all pages matching the pattern
			"js":["jquery-1.11.2.min.js", "key-interceptor.js", "scraper.js"] // .js files to inject
		}
	],
    
    "permissions": [
        "tabs"
    ]
}